{
  "name": "create-fastify-boot",
  "version": "1.0.2",
  "description": "Set up a Fastify Boot app with one command.",
  "repository": "https://github.com/burketyler/create-fastify-boot",
  "author": "Tyler Burke",
  "main": ".build/index.js",
  "license": "MIT",
  "files": [
    ".build",
    "template",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-fastify-app": ".build/cli.js"
  },
  "scripts": {
    "clean": "rm -rf .build",
    "build": "yarn clean && tsc && chmod +x .build/cli.js",
    "cm": "git-cz"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^9.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "commitizen": "^4.2.3",
    "commitlint": "^12.0.1",
    "cz-customizable": "^6.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
